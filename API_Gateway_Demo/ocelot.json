{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/categories/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "category-service",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/categories/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "categories"
    },
    {
      "DownstreamPathTemplate": "/api/products/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "product-service",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/products/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "products"
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5222"
  },
  "SwaggerEndPoints": [
    {
      "Key": "categories",
      "Config": [
        {
          "Name": "CategoryService v1",
          "Version": "v1",
          "Url": "http://category-service:8080/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "products",
      "Config": [
        {
          "Name": "ProductService v1",
          "Version": "v1",
          "Url": "http://product-service:8080/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}